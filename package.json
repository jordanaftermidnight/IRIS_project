{
  "name": "iris-ai",
  "version": "0.9.0",
  "description": "Iris - Integrated Runtime Intelligence Service with smart provider selection, supporting Ollama, Gemini, OpenAI, Groq, and Anthropic",
  "main": "src/enhanced-ai.js",
  "type": "module",
  "bin": {
    "iris": "./src/cli.js"
  },
  "preferGlobal": true,
  "scripts": {
    "start": "node src/enhanced-ai.js",
    "demo": "node start-enhanced-demo.cjs",
    "demo:simple": "node demo-start.cjs",
    "api": "node iris-api-server.js",
    "test": "echo 'No tests configured - tests removed for production'",
    "test:examples": "node examples/basic-usage.js",
    "dev": "node --watch src/enhanced-ai.js",
    "health": "node src/cli.js health",
    "models": "node src/cli.js models", 
    "providers": "node src/cli.js providers",
    "cache-stats": "node src/cli.js cache-stats",
    "pool-stats": "node src/cli.js pool-stats",
    "performance": "node src/cli.js performance",
    "postinstall": "node setup.js",
    "setup": "node setup.js",
    "lint": "eslint src/ examples/",
    "lint:fix": "eslint src/ examples/ --fix",
    "validate": "npm run lint && npm test",
    "prepack": "npm run validate"
  },
  "keywords": [
    "ai",
    "llm",
    "ollama",
    "gemini",
    "anthropic",
    "cli",
    "multi-provider",
    "automation",
    "machine-learning",
    "nlp",
    "chat"
  ],
  "author": "Jordan After Midnight",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanaftermidnight/IRIS_project.git"
  },
  "bugs": {
    "url": "https://github.com/jordanaftermidnight/IRIS_project/issues"
  },
  "homepage": "https://github.com/jordanaftermidnight/IRIS_project#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google/generative-ai": "^0.21.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "groq-sdk": "^0.26.0",
    "ollama": "^0.5.16",
    "openai": "^5.8.2"
  },
  "devDependencies": {
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jordanaftermidnight"
  }
}
